<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
           version="3.0">

    <session-config>
        <session-timeout>3</session-timeout>
    </session-config>


    <servlet>
        <servlet-name>WelcomeServlet</servlet-name>
        <servlet-class>pack1.WelcomeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>WelcomeServlet</servlet-name>
        <url-pattern>/WelcomeServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Servlet1</servlet-name>
        <servlet-class>pack1.Servlet1</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Servlet1</servlet-name>
        <url-pattern>/Servlet1</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ServletP</servlet-name>
        <servlet-class>ServletP</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletP</servlet-name>
        <url-pattern>/ServletP</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ServletP2</servlet-name>
        <servlet-class>ServletP2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletP2</servlet-name>
        <url-pattern>/ServletP2</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Servlet2</servlet-name>
        <servlet-class>pack1.Servlet2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Servlet2</servlet-name>
        <url-pattern>/Servlet2</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>pack1.Logout</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>
</web-app>
